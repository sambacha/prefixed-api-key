{
  "author": {
    "name": "Glenn Rempe",
    "url": "https://www.truestamp.com/",
    "email": "glenn@truestamp.com"
  },
  "bugs": {
    "url": "https://github.com/truestamp/prefixed-api-key/issues"
  },
  "contributors": [
    {
      "name": "seam",
      "url": "https://www.seam.co/"
    }
  ],
  "description": "Create and verify secure prefixed API keys",
  "devDependencies": {
    "@scure/base": "^1.1.1",
    "@skypack/package-check": "^0.2.2",
    "@stablelib/base64": "^2.0.0",
    "@stablelib/bytes": "^2.0.0",
    "@stablelib/hmac": "^2.0.0",
    "@stablelib/random": "^2.0.0",
    "@stablelib/sha256": "^2.0.0",
    "@stablelib/utf8": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/coverage-v8": "^2.0.4",
    "eslint": "^9.7.0",
    "tsup": "^8",
    "typescript": "^5.5.4",
    "ulidx": "^2.3.0",
    "vitest": "^2.0.4",
    "zod": "^3.21.4",
    "zod-validation-error": "^3.3.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "typings": "./dist/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "/src",
    "/dist",
    "/examples",
    "/tests"
  ],
  "homepage": "https://github.com/truestamp/prefixed-api-key",
  "keywords": [
    "nodejs",
    "deno",
    "api",
    "key",
    "token",
    "security",
    "random",
    "hash",
    "sha256",
    "hmac",
    "base58",
    "base58check",
    "crypto"
  ],
  "license": "MIT",
  "name": "@truestamp/prefixed-api-key",
  "repository": "git@github.com:truestamp/prefixed-api-key.git",
  "scripts": {
    "bench": "vitest bench",
    "build": "tsup src/index.ts --clean --format esm,cjs --dts",
    "clean": "rm -rf dist",
    "examples:deno": "cd examples/deno && deno run ./index.ts",
    "examples:nodejs": "cd examples/nodejs && node ./index.cjs",
    "lint": "eslint \"src/**/*.ts*\"",
    "package-check": "npx package-check",
    "test": "vitest --coverage",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --coverage"
  },
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "version": "2.0.0"
}
